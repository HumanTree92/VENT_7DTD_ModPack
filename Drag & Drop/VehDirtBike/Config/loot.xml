<configs>
	<insertBefore xpath="/lootcontainers/lootgroup[@name='groupScrapCommon']">
		<lootgroup name="DirtBikegroup">
			<item name="vehicleDirtBikeChassis"/>
			<item name="vehicleDirtBikeParts"/>
		</lootgroup>
	</insertBefore>
	<append xpath="/lootcontainers/lootgroup[@name='vehiclePartsTiered']">
		<item group="DirtBikegroup" loot_prob_template="ProbT2"/>
	</append>
	<append xpath="/lootcontainers/lootgroup[@name='schematicsVehiclesT1']">
		<item name="vehicleDirtBikeChassisSchematic"/>
		<item name="vehicleDirtBikePartsSchematic"/>
	</append>
</configs>