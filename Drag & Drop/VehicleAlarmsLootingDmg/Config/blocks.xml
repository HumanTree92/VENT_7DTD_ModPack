<blocks>

	<!-- 
	Extends
	backhoeWhite
	cntAmbulanceTruck01
	cntArmyTruckClosed
	cntBoxTruckWhite
	cntBusSchool
	cntBusSchoolShort
	cntBusShuttle
	cntBusCity
	cntServiceTruckMoPower
	cntServiceTruckWorkingStiffTools
	cntServiceTruckWhite
	excavatorClawWhite
	excavatorWhite
	tractorWhite
	-->
	<append xpath="//block[@name='cntBusSchool']">
		<property name="Tags" value="vehicleDamageAlarm"/>
	</append>

	<!-- #region - No Damage Alarm Vehicles -->
		<append xpath="//block[@name='backhoeWhite']">
			<property name="Tags" value="vehicleDamageNoAlarm"/>
		</append>

		<append xpath="//block[@name='excavatorClawWhite']">
			<property name="Tags" value="vehicleDamageNoAlarm"/>
		</append>

		<append xpath="//block[@name='excavatorWhite']">
			<property name="Tags" value="vehicleDamageNoAlarm"/>
		</append>
		
		<append xpath="//block[contains(@name, 'tractor')]">
			<property name="Tags" value="vehicleDamageNoAlarm"/>
		</append>
	<!-- #endregion - No Damage Alarm Vehicles -->

	<!-- FIRETRUCKS -->
	<append xpath="//block[@name='cntFireTruck01White']">
